doctype html
html
  head
    meta(name="viewport", content="width=device-width")
    link(href="https://fonts.googleapis.com/icon?family=Material+Icons", rel="stylesheet")
    link(rel='stylesheet', href='/css/style.css')
    script(src='/socket.io/socket.io.js')
    script(src='https://code.jquery.com/jquery-2.2.2.min.js',integrity='sha256-36cp2Co+/62rEAAYHLmRCPIych47CvdM+uTBJwSzWjI=', crossorigin='anonymous')
    script(src='/js/script.js')
    title rapidpoll
  body
    #container
      h1 rapidpoll
      
      #question-div
        
        h2#question loading...
        #timer
        
      #input-container
        .input-div
          input#answer-input(type='text', disabled, placeholder='no question to answer')
          .submit-div
            a#answer-submit
              i.material-icons send

        .input-div
          input#question-input(type='text', placeholder='ask a question')
          .submit-div
            a#question-submit
              i.material-icons send
        
        #info
          p#online
          p#queue
          
        
          
      
      
          
      #answer-list
        ul
      
      p#links
            a(href='https://github.com/williamyeny/rapidpoll',target='_blank')
              i.material-icons code
            a(onclick='document.getElementById(\'paypal\').submit();')
              i.material-icons attach_money
            <form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_blank" id="paypal">
            <input type="hidden" name="cmd" value="_s-xclick">
            <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----
            ">
            <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">
            </form>
            
      #ad
        script(async, src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js")
        ins.adsbygoogle(style="display:block", data-ad-client="ca-pub-6411505887325343", data-ad-slot="9725860914", data-ad-format="auto")
        
        script (adsbygoogle = window.adsbygoogle || []).push({});